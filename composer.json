{
    "name": "josecanciani/fat",
    "version": "0.1.0",
    "autoload": {
        "psr-4": {
            "Josecanciani\\Fat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josecanciani\\Fat\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jose Canciani",
            "email": "github@jluis.com.ar"
        }
    ],
    "require": {
        "php": "^8.1",
        "theodo-group/llphant": "^0.10.1",
        "symfony/console": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests/Unit",
        "test:integration": "./vendor/bin/phpunit tests/Integration"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
